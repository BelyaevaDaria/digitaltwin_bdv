Electrical_Temperature_format_options = units.GetFormatOptions(Electrical_Temperature_unit_type)
Electrical_Temperature_format_options.UnitSymbol = UnitSymbolType.UST_NONE
units.SetFormatOptions(Electrical_Temperature_unit_type, Electrical_Temperature_format_options)
doc.SetUnits(units)

